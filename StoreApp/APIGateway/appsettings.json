{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:5000",
  "ReverseProxy": {
    "Routes": {
      "usersRoute": {
        "ClusterId": "usersCluster",
        "MatchCondition": {
          "Path": "/api/users/{*}"
        }
      }
    },
    "Clusters": {
      "usersCluster": {
        "Destinations": {
          "usersDestination": {
            "Address": "http://localhost:5001/"
          }
        }
      }
    }
  }
}
